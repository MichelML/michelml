{
  "name": "michelml",
  "version": "1.0.0",
  "description": "personal website and beyond",
  "main": "index.js",
  "scripts": {
    "build": "npm run blog && npm run library && next build",
    "now-build": "npm run blog && npm run library && next build",
    "now-start": "next start",
    "start": "next start",
    "dev": "next dev",
    "lint": "prettier --write 'components/**/*.js' 'pages/**/*.js' '!pages/library/**/*.js' '!pages/blog/**/*.js' 'hoc/**/*.js' 'transitions/**/*.js' 'blog_posts/**/*.js' 'utils/**/*.js' 'scripts/**/*.js'",
    "cpost": "node ./scripts/blog_posts/create_post.js",
    "aposts": "node ./scripts/blog_posts/aggregate_posts.js",
    "pposts": "node ./scripts/blog_posts/pages_posts.js",
    "blog": "npm run aposts && npm run pposts",
    "findbook": "node ./scripts/books/find_book.js",
    "abooks": "node ./scripts/books/aggregate_books.js",
    "pbooks": "node ./scripts/books/pages_books.js",
    "library": "npm run abooks && npm run pbooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichelML/michelml.git"
  },
  "author": "Michel Moreau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichelML/michelml/issues"
  },
  "homepage": "https://github.com/MichelML/michelml#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "next": "^8.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-transition-group": "^2.8.0",
    "striptags": "^3.1.1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "deepai": "^1.0.0",
    "image-downloader": "^3.4.2",
    "prettier": "1.16.4",
    "svg-to-img": "^2.0.9",
    "trianglify": "^2.0.0",
    "url-join": "^4.0.0",
    "yargs": "^13.2.2"
  }
}
